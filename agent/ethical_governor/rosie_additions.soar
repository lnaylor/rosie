sp {governor*elaborate*problem-space
	(state <s> ^superstate.name governor-selection)
-->
	(<s> ^problem-space <ps>
		^default-problem-space no)
	(<ps> ^name action
			^dont-copy action-stack
			^execution-type internal
			^world <world>
			^action <action>)}

sp {pop*failed*actions
	(state <s> ^operator <o> + ^top-state <ts>)
	(<ts> ^action-stack.changes <c> ^operator <exec-op>)
	(<o> ^evaluated failure -^name dummy)
	(<exec-op> ^action <a>)
-->
	(<c> ^pop <a>)}

sp {pop*left-over*successes
	(state <s> ^superstate nil ^action-stack <a-stack> ^operator.name satisfy-purpose)
	(<a-stack> ^actions.action <act> ^changes <c>)
	(<act> ^execution-operator.evaluated success)
-->
	(<c> ^pop <act>)}
