sp {elaborate*item*eval
	:o-support
	(state <s> ^name selection
				^evaluation <e>)
	(<e> ^superoperator <i>
			^symbolic-value <sym>)
	(<i> -^name dummy)
-->
	(<i> ^evaluated <sym>)}

sp {elaborate*success
	(state <s> ^desired <d> ^constraint <c>)
	-(<s> ^constraint (-^violated no))
	-(<s> ^constraint (-^violated { <something> }))
	-(<s> ^constraint (^violated { <> no <other> }))
-->
	(<s> ^success <d>)}

sp {elaborate*failure
	(state <s> ^constraint.violated yes
				^desired <d>)
-->
	(<s> ^failure <d>)}

