sp {elaborate*item*eval
	:o-support
	(state <s> ^name governor-selection
				^evaluation <e>)
	(<e> ^superoperator <i>
			^symbolic-value <sym>)
	(<i> -^name dummy)
-->
	(<i> ^eval <sym>)}

sp {elaborate*evaluated
	:o-support
	(state <s> ^operator <o> +)
	(<o> ^eval { <> exhaustion-failure <sym> })
-->
	(<o> ^evaluated <sym>)}

sp {elaborate*success
	(state <s> ^name evaluate-operator ^constraint <c> ^superstate <ss>)
	(<ss> ^name governor-selection ^operator <o>)
	(<o> ^name evaluate-operator ^evaluation <e>)
	-(<s> ^constraint (-^violated no))
	-(<s> ^constraint (-^violated { <something> }))
	-(<s> ^constraint (^violated { <> no <other> }))
-->
	(<e> ^symbolic-value success)}

sp {elaborate*failure
	(state <s> ^constraint.violated yes
				^name evaluate-operator ^superstate <ss>)
	(<ss> ^name governor-selection ^operator <o>)
	(<o> ^name evaluate-operator ^evaluation <e>)
-->
	(<e> ^symbolic-value failure)}
