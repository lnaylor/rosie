sp {learn-proposal-rule*elaborate*match-action
   (state <s> ^name learn-proposal-rule
              ^reference-operator <o>)
-->
   (<s> ^match-action.action <o>)
}

sp {learn-proposal-rule*elaborate*match-action*candidate-action
   (state <s> ^name learn-proposal-rule
              ^operator <o> +
              ^match-action <ma>)
-->
   (<ma> ^candidate-action <o>)
}
