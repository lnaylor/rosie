sp {action-space*retrospective*selection*elaborate*match-action
   (state <s> ^name selection
              ^problem-space.action-space retrospective
              ^subactions.next <sub>)
-->
   (<s> ^match-action.action <sub>)
}

sp {action-space*retrospective*selection*elaborate*match-action*candidate-action
   (state <s> ^name selection
              ^problem-space.action-space retrospective
              ^item <a>
              ^match-action <ma>)
-->
   (<ma> ^candidate-action <a>)
}