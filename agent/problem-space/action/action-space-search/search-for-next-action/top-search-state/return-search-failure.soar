sp {search-for-next-action*top-search-state*propose*return-search-failure
   (state <s> ^problem-space.action-space search
              ^top-search-state true
              ^action-search-failed true)
-->
   (<s> ^operator <o> + >)
   (<o> ^name return-search-failure)
}

sp {search-for-next-action*top-search-state*apply*return-search-failure*report*action-search-failed*superstate
   (state <s> ^problem-space.action-space search
              ^top-search-state true
              ^operator.name return-search-failure
              ^superstate <ss>)
-->
   (<ss> ^action-search-failed true)
}
