sp {op_put-down1*fetch*apply*complete-action*remove*grabbed
   (state <s> ^name op_put-down1
              ^operator <o>
              ^held-object.predicates <preds>)
   (<o> ^name complete-action
        ^status success)
   (<preds> ^arm-status grabbed)
-->
   (<preds> ^arm-status grabbed -
            ^arm-status not-grabbed)
}

sp {op_put-down1*fetch*apply*complete-action*remove*put-down-command
   (state <s> ^name op_put-down1
              ^operator <o>
              ^current-action <a>
              ^io.output-link <out>)
   (<o> ^name complete-action
        ^status success)
   (<a> ^put-down-command <cmd>)
   (<out> ^put-down <cmd>)
-->
   (<a> ^put-down-command <cmd> -)
   (<out> ^put-down <cmd> -)
}
